@startuml
skinparam groupInheritance 2

class Library {
+ name: str
+ address: str
}

class Book{
+ tittle: str
+ pages: int
+ release: date
}

class Author {
+ name: str
+ email: str 
}

class Institution {
+ name: str
}

Book "*" -- "1..*" Author: writedBy
Library "1" -- "*" Book: has
Author "*" -- "1" Institution: work

@enduml